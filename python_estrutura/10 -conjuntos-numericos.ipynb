    "\n",
    "a = {1, 2, 3}\n",
    "b = {2, 3, 4}\n",
    "\n",
    "#união de conjutos\n",
    "print(a.union(b))\n",
    "\n",
    "#interceção\n",
    "print(a.intersection(b))\n",
    "\n",
    "#contido\n",
    "print(a <= b)#false\n",
    "print({4} <= b)#true\n",
    "\n",
    "#direferença\n",
    "c = a - b#o que tem em a que não tem em b\n",
    "print(c)\n",
    "c = b - a # vice-versa\n",
    "print(c)"
